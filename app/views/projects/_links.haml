.links
  - section.links.each do |snippet|
    .link
      .title= link_to snippet.title, url_with_protocol(snippet.url), target: '_blank'
      .excerpt= snippet.excerpt
      = link_to project_section_link_path(@project, section, snippet), name: 'delete-link', method: 'delete' do
        %i.fa.fa-close
#add-link-link 
  = link_to new_project_section_link_path(@project, section) do
    %i.fa.fa-plus
    Add
